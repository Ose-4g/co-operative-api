    "lint": "eslint '*/**/*.{ts,tsx}' --quiet --fix",
    "format": "prettier --write src/",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write"
    ]
