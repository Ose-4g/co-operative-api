  "main": "index.js",
  "scripts": {
    "heroku-prebuild": "npm i -f",
    "build": "rimraf dist && tsc",
    "dev": "nodemon",
    "start": "node dist/server.js",
    "test": "NODE_ENV=test npm run build && mocha dist --timeout 25000 --recursive  --exit",
