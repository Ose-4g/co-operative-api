    "@types/node": "^17.0.18",
    "@types/nodemailer": "^6.4.4",
    "@types/validator": "^13.7.1",
    "chai": "^4.3.6",
    "eslint": "^8.9.0",
    "lint-staged": "^12.3.4",
    "mocha": "^9.2.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "sequelize-cli": "^6.4.1",
    "typescript": "^4.5.5"
  }
